{
  "schemaVersion": 1,
  "id": "cooparticlesapi",
  "version": "${version}",
  "name": "CooParticleAPI",
  "description": "CooParticlesAPI made by CooStack",
  "authors": [
    "CooStack"
  ],
  "contact": {
    "homepage": "https://space.bilibili.com/291397844",
    "sources": "https://github.com/CooStack/CooParticlesAPI"
  },
  "license": "CC0-1.0",
