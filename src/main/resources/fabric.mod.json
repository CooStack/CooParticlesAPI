{
	"schemaVersion": 1,
	"id": "cooparticleapi",
	"version": "${version}",
	"name": "CooParticleAPI",
	"description": "此插件会修改粒子个数上限(使用mixin) 如果存在其他插件也使用了类似的方法可能会冲突",
	"authors": [
		"CooStack"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/cooparticleapi/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "cn.coostack.CooParticleAPI",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "cn.coostack.CooParticleAPIDataGenerator",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "cn.coostack.CooParticleAPIClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"cooparticleapi.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}